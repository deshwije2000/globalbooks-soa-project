<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03"
        xmlns:bpel="http://globalbooks.com/bpel"
        xmlns:cat="http://globalbooks.com/catalog">
    <process name="bpel:PlaceOrder">
        <active>true</active>
        <provide partnerLink="client">
            <service name="bpel:PlaceOrderService" port="PlaceOrderPort"/>
        </provide>
        <invoke partnerLink="catalogService">
            <service name="cat:CatalogService" port="CatalogPort"/>
        </invoke>
    </process>
</deploy>